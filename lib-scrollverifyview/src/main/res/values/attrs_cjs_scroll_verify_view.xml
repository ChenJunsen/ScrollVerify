<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScrollVerifyView">
        <attr name="openRandomRotate" format="boolean"/>
        <attr name="openRandomBlockLocation" format="boolean"/>
        <attr name="blockWidth" format="dimension"/>
        <attr name="blockHeight" format="dimension"/>
        <attr name="blockWidthDivide" format="integer"/>
        <attr name="blockHeightDivide" format="integer"/>
        <attr name="viewPadding" format="dimension"/>
        <attr name="movableBlockDrawable" format="reference"/>
        <attr name="targetBlockDrawable" format="reference"/>
        <attr name="maxScrollDistance" format="dimension"/>
        <attr name="blockTouchable" format="boolean"/>
        <attr name="validOffset" format="dimension"/>
        <attr name="rotateDegree" format="float"/>
        <attr name="avoidGenerateTooClose" format="boolean"/>
        <attr name="scrollMode">
            <flag name="only_horizontal" value="1"/>
            <flag name="only_vertical" value="2"/>
            <flag name="only_free" value="3"/>
            <flag name="free_horizontal" value="4"/>
            <flag name="free_vertical" value="5"/>
        </attr>
    </declare-styleable>
</resources>
